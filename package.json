      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "next": "^13.2.4",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-globe.gl": "^2.17.1",
    "react-is": "^18.2.0",
    "react-simple-maps": "^3.0.0",
